<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.teamnexters.zaza">

    <application
            android:name=".ZazaApplication"
            android:allowBackup="true"
            android:icon="@mipmap/ic_launcher"
            android:label="@string/app_name"
            android:roundIcon="@mipmap/ic_launcher_round"
            android:supportsRtl="true"
            android:theme="@style/AppTheme">
        <activity android:name=".ui.dream.DreamDetailActivity"/>

        <activity android:name=".sample.firebase.DatabaseActivity">
        </activity>
        <activity android:name=".sample.firebase.ImageActivity">
        </activity>
        <activity android:name=".ui.dream.DreamActivity">
        </activity>
        <activity android:name=".ui.alarm.AlarmActivity">
           
        </activity>
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>
                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <receiver android:name=".util.alarm.AlarmReceiver">
        </receiver>

    </application>

</manifest>